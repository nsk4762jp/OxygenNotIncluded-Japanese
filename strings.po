msgctxt "STRINGS.ELEMENTS.STATE.GAS"
msgid "Gas"
msgstr "気体"

#. STRINGS.ELEMENTS.STATE.LIQUID
msgctxt "STRINGS.ELEMENTS.STATE.LIQUID"
msgid "Liquid"
msgstr "液体"

